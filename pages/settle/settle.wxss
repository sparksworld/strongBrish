.settle {
    position: absolute;
    width: 100%;
    height: 100%;
}
.settle .head_status{
    width: 100%;
    display: flex;
    justify-content: center;
    padding: 20rpx 0;
}
.settle .head_status image{
    width: 363rpx;
}
.settle .bothInfo{
    width: 100%;
    height: 200rpx;
    background: rgba(64, 152, 152, .8);    
    display: flex;
    justify-content: space-around;
    box-shadow: 0rpx 10rpx 10rpx #888888;
}
.settle .bothInfo .info{
    display: flex;
    /* align-content: space-around; */
    align-items: center;
}
.settle .bothInfo .info image{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background: #ccc;
}
.settle .bothInfo .info .textInfo{
    display: flex;
    flex-direction: column;
    font-size: 30rpx;
    color: #fff;
    margin-left: 18rpx;
}
.leftline{
    border: 1px solid;
}
.btns {
    display: flex;
    justify-content: space-around;
}
.btns button{
    margin: 30rpx 0;
    line-height: 80rpx;
    width: 40%;
}
.challagheDetail {
    width: 100%;
    line-height: 60rpx;
}
.challagheDetail .challageTitle{
    display: flex;
    width: 100%;
    justify-content: center;
}
.challagheDetail text{
    position: relative;
    font-size: 30rpx;
    color: #444;
}
.ChakkageGrade{
    width: 100%;
    height: 300rpx;
    /* background: #444; */
}
.ChakkageGrade>view {
    display: flex;
    justify-content: space-around;
    border-bottom: 1px solid #ccc;    
    font-size: 30rpx;
    color: #444;
    overflow: hidden;
}
.ChakkageGrade .table view{
    width: 100%;
    font-size: 30rpx;
    line-height: 65rpx;
    text-align: center;
}